// very important question I get to know where and how to use set and reduse time complexity of programs 
// It is must to see all submissions of this question to know difference between all the submission.
// question link ======> https://codeforces.com/contest/274/problem/A
// submission link =======>  https://codeforces.com/contest/274/submission/118136267
#include<iostream>
#include<bits/stdc++.h>
#define int long long
using namespace std;
signed main()
{
    int n,k;
    cin>>n>>k;
    int i,no;
    set<int>s;
    for(i=0;i<n;i++)
    {
        cin>>no;
        s.emplace(no);
    }
    if(k > 1)
    {
        for(auto it : s)
        {
            s.erase(k*it);
        }
    }
    
    cout<<s.size()<<"\n";
    return 0;
}
